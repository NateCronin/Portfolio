-- Get Schema of tables

SELECT 
   column_name, data_type
 FROM 
  `coffeking-dataset.yelp_dataset`.INFORMATION_SCHEMA.COLUMNS
WHERE 
  table_name = 'Business';


SELECT 
   column_name, data_type
 FROM 
  `coffeking-dataset.yelp_dataset`.INFORMATION_SCHEMA.COLUMNS
WHERE 
  table_name = 'User';

SELECT 
   column_name, data_type
 FROM 
  `coffeking-dataset.yelp_dataset`.INFORMATION_SCHEMA.COLUMNS
WHERE 
  table_name = 'review';
